  "scripts": {
    "build": "tsc",
    "start": "tsc && concurrently -n 'FRNT,BKND,CLNT' -c 'red,green,blue' --kill-others 'yarn cli:frontend' 'yarn cli:backend' 'yarn cli:client'",
    "cli:client": "node ./dist/cli.js client 'ws://localhost:9001' 'wWbHNsjWPhqUo2etM6vc3qedoKO9jgYLklql1h4vzf3AKWrvCFe38/PBSLZNgmC1igGAXTq6axEb64S95AMeBAAQdGVzdC5pb2ZzaGl0LmNvbQAAAAA='",
    "cli:frontend": "node ./dist/cli.js frontend",
    "cli:backend": "node ./dist/cli.js backend 'JGmVGhoC+ozGC3b+KuzGwtZfWehcMVKa2YfuG9Kd01A='",
    "key": "tsc && node ./dist/newKey.js"
  },
  "dependencies": {
    "@types/commander": "^2.12.2",
    "@types/express": "4.17.2",
    "@types/pino": "^5.15.5",
    "@types/uuid": "7.0.0",
    "@types/ws": "7.2.2",
    "commander": "^4.1.1",
    "express": "4.17.1",
    "pino": "^5.17.0",
    "pino-pretty": "^3.6.0",
    "ts-nats": "1.2.4",
    "tweetnacl": "^1.0.3",
    "typescript": "3.8.3",
    "uuid": "7.0.1",
    "ws": "7.2.1"
  },
  "devDependencies": {
    "concurrently": "5.1.0"
  }
