  },
  "dependencies": {
    "@types/express": "4.17.2",
    "@types/pino": "^5.15.5",
    "@types/uuid": "7.0.0",
    "@types/ws": "7.2.2",
    "express": "4.17.1",
    "pino": "^5.17.0",
    "pino-pretty": "^3.6.0",
    "ts-nats": "1.2.4",
    "tweetnacl": "^1.0.3",
    "typescript": "3.8.3",
    "uuid": "7.0.1",
    "ws": "7.2.1"
  },
  "devDependencies": {
    "concurrently": "5.1.0"
  }
}