    "cli:client": "node ./dist/cli.js client 'ws://localhost:9001' 'wWbHNsjWPhqUo2etM6vc3qedoKO9jgYLklql1h4vzf3AKWrvCFe38/PBSLZNgmC1igGAXTq6axEb64S95AMeBAAQdGVzdC5pb2ZzaGl0LmNvbQAAAAA='",
    "cli:frontend": "node ./dist/cli.js frontend",
    "cli:backend": "node ./dist/cli.js backend 'JGmVGhoC+ozGC3b+KuzGwtZfWehcMVKa2YfuG9Kd01A='",
    "cli:test": "node ./dist/cli.js client 'wss://backhaul.orcarium.com' '9i5VkYAejdy7kIzEGARaq9n9hqd9mMSqkRuS4hJHEwJYQH0KRqGPs6hLj77jDp1nhBsbI6bULaAt4+09b9X3CwARdGVzdC5vcmNhcml1bS5jb20AAAAA'",
    "key": "tsc && node ./dist/newKey.js",
    "docker": "./scripts/increment_version.js && ./scripts/docker.sh"
  },
  "dependencies": {
    "@types/commander": "^2.12.2",
    "@types/express": "4.17.2",
    "@types/node-fetch": "^2.5.5",
